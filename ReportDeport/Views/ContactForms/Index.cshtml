            @Html.DisplayFor(modelItem => item.message)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.subject)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.conpanyEmail)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ContactFormId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ContactFormId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ContactFormId })
        </td>
    </tr>
}

</table>
